<%= path %> {
  daily
  missingok
  rotate <%= defined?(count) ? count : 10 %>
  compress
  size 10M
  delaycompress
  sharedscripts
  <%= "create #{create}" if defined?(create) %>
  <%= "prerotate\n    #{pre_rotate}\n  endscript" if defined?(pre_rotate) %>
  <%= "postrotate\n    #{post_rotate}\n  endscript" if defined?(post_rotate) %>
}
